<p id="notice"><%= notice %></p>

<h1>Premios</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Ano</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @premios.each do |premio| %>
      <tr>
        <td><%= premio.nome %></td>
        <td><%= premio.ano %></td>
        <td><%= premio.user_id %></td>
        <td><%= link_to 'Show', premio %></td>
        <td><%= link_to 'Edit', edit_premio_path(premio) %></td>
        <td><%= link_to 'Destroy', premio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Premio', new_premio_path %>
